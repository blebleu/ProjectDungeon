    DestX= Target.x;
    DestY= Target.y;
    mp_potential_step(DestX,DestY,spd,false);
}
else{
    Target.jumpscareTimer++;
    if(Target.jumpscareTimer&gt;Target.jumpscareLimit || point_distance(x,y,Target.x,Target.y)&lt;Target.jumpscareProximity){
        Target.jumpscare=true;
        Target.jumpscareTime=Target.jumpscareDur;
        x=6000;
        y=6000;
        teleportTimer=-10*room_speed;
    }
}
if(point_distance(x,y,Target.x,Target.y)&gt;=scanLen+50 &amp;&amp; teleportTimer&gt;teleportLimit){
